<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user[1] %> - <%= user[0] %></td>
        <td><%= link_to 'Events', "/apps/#{params[:bundle]}/users/#{user[0]}/events" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
